{
  "name": "wyrihaximus/ratchet-model-push",
  "description": "Allows models to push data on write operations to wyrihaximus/ratchet.",
  "type": "cakephp-plugin",
  "keywords": [
    "cakephp",
    "ratchet"
  ],
  "homepage": "http://github.com/WyriHaximus/RatchetModelPush",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan kiewiet",
      "email": "ceesjank@gmail.com",
      "role": "Maintainer",
      "homepage": "http://wyrihaximus.net/"
    }
  ],
  "require": {
    "composer/installers": "*",
    "wyrihaximus/ratchet": "^0.1",
    "wyrihaximus/ratchet-commands": "^0.1"
  },
  "support": {
    "issues": "https://github.com/WyriHaximus/RatchetModelPush/issues",
    "source": "https://github.com/WyriHaximus/RatchetModelPush"
  },
  "extra": {
    "installer-name": "RatchetModelPush"
  },
  "config": {
    "vendor-dir": "Vendor"
  }
}
